@model IEnumerable<CrmWebApp.Models.CompanyMeeting>

@{
    ViewBag.Title = "Index";
}

<h2>@ViewBag.CompanyName</h2>

<p>
    @Html.ActionLink("Create New", "Create", new { companyId = ViewBag.CompanyId })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CompanyId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CompanyName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MeetingType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MeetDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MeetAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MeetNames)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreateUserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreateTime)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CompanyId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompanyName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MeetingType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MeetDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MeetAddress)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MeetNames)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreateUserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreateTime)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
